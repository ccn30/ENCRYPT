resultsDir <- '/home/ccn30/rds/hpc-work/WBIC_lustre/ENCRYPT/results/PIT'
scriptDir <- '/home/ccn30/rds/hpc-work/WBIC_lustre/ENCRYPT/scripts/PIT'
